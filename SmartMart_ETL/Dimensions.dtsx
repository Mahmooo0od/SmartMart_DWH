      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{CF5F7E39-D063-48F4-92B3-90D4CEC8DF7A}"
          SQLTask:SqlStatementSource="SET NOCOUNT ON;&#xA;SET XACT_ABORT ON;&#xA;&#xA;BEGIN TRY&#xA;  BEGIN TRAN;&#xA;&#xA;  ;WITH src AS (&#xA;    SELECT&#xA;      UPPER(LTRIM(RTRIM(BranchCode))) AS BranchCode,&#xA;      MAX(BranchName) AS BranchName,&#xA;      MAX(City)       AS City,&#xA;      MAX(Region)     AS Region&#xA;    FROM stg.Branch_stage&#xA;    WHERE BranchCode IS NOT NULL&#xA;    GROUP BY UPPER(LTRIM(RTRIM(BranchCode)))&#xA;  )&#xA;  MERGE dwh.DimBranch AS tgt&#xA;  USING src&#xA;     ON tgt.BranchCode = src.BranchCode&#xA;  WHEN MATCHED THEN&#xA;      UPDATE SET&#xA;        tgt.BranchName = src.BranchName,&#xA;        tgt.City       = src.City,&#xA;        tgt.Region     = src.Region&#xA;  WHEN NOT MATCHED THEN&#xA;      INSERT (BranchCode, BranchName, City, Region)&#xA;      VALUES (src.BranchCode, src.BranchName, src.City, src.Region);&#xA;&#xA;  TRUNCATE TABLE stg.Branch_stage;&#xA;&#xA;  COMMIT TRAN;&#xA;END TRY&#xA;BEGIN CATCH&#xA;  IF @@TRANCOUNT &gt; 0 ROLLBACK TRAN;&#xA;  THROW;&#xA;END CATCH;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
